[N,T] = [int(c) for c in input().split()]




V = input("vertices: ").split()
grafo = []

for i in range(len(V)):
    
    grafo.append({V[i] : []})
    print("Vertice ",V[i])
    PesoVertice = entrada()
    while(PesoVertice != "0"):
        grafo[i][V[i]].append(PesoVertice)
        PesoVertice = entrada()

print(grafo)
